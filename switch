#include <iostream>

void task_1() {
    std::cout << "enter num of week-day: ";
    int week_day = 0;
    std::cin >> week_day;
    
    const int Fr = 5;
    
    switch (week_day) {
        case 1:
            std::cout << "Md" << std::endl;
            
            break;
        case 2:
            std::cout << "Th" << std::endl;
            break;
        case 3:
            std::cout << "Wn" << std::endl;
            break;
        case 4:
            std::cout << "Th" << std::endl;
            break;
        case 5:
            std::cout << "Fr" << std::endl;
            break;
        case 6:
            std::cout << "Sd" << std::endl;
            break;
        case 7:
            std::cout << "Sun" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        
   }
    
}

void task_2(){
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    switch (sym) {
        case '+':
            std::cout << "+plus" << std::endl;
            break;
        case '-':
            std::cout << "minys-" << std::endl;
            break;
        case '*':
            std::cout << "*ymnozh" << std::endl;
            break;
        case '/':
            std::cout << "/podelit" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        }
    
}

void task_3(){
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    switch (sym) {
        case '5':
            std::cout << "otlichno" << std::endl;
            break;
        case '4':
            std::cout << "horosho" << std::endl;
            break;
        case '3':
            std::cout << "*norm" << std::endl;
            break;
        case '2':
            std::cout << "ploho" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        }
    
}

void task_4() {
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    std::cout << "enter num_0: ";
    int num_0;
    std::cin >> num_0;
    
    std::cout << "enter num_1: ";
    int num_1;
    std::cin >> num_1;
    
    
    switch (sym) {
        case '+':
            std::cout << num_0 + num_1 << std::endl;
            break;
        case '-':
            std::cout << num_0 - num_1 << std::endl;
            break;
        case '*':
            
            std::cout << num_0 * num_1 << std::endl;
            break;
        case '/':
            
            std::cout << num_0 / num_1 << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
    }
    
}

void task_6(){
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    switch (sym) {
        case 'R':
            std::cout << "Russia, Romania" << std::endl;
            break;
        case 'U':
            std::cout << "Uzbekistan" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        }
    
}




void task_10(){
    char symbol;
    
    
    std::cout << "vvedi simvol: ";
    std::cin >> symbol;
    
    
    switch (symbol) {
        // Проверка на цифры от 0 до 9
        case '0':
        case '1':
        case '2':
        case '3':
        case '4':
        case '5':
        case '6':
        case '7':
        case '8':
        case '9':
            std::cout << "yavl cifroy" << std::endl;
            break;
            
        
        case 'a':
        case 'b':
        case 'c':
        case 'd':
        case 'e':
        case 'f':
        case 'g':
        case 'h':
        case 'i':
        case 'j':
        case 'k':
        case 'l':
        case 'm':
        case 'n':
        case 'o':
        case 'p':
        case 'q':
        case 'r':
        case 's':
        case 't':
        case 'u':
        case 'v':
        case 'w':
        case 'x':
        case 'y':
        case 'z':
            std::cout << "strochn" << std::endl;
            break;
            
        default:
            std::cout << "drygoy simvol" << std::endl;
            break;
    }
    
    
}



void task_11() {
    int number;
    
    std::cout << "celoe chislo: ";
    std::cin >> number;
    
    std::string result = (number % 2 == 0) ? "chet" : "ne chet";
    
    std::cout << "vvedennoe chisl yavlaetsya " << result << std::endl;
    

}

void task_12() {
    
    int num1, num2, max;
    
    std::cout << "vvedite pervoe chislo: ";
    std::cin >> num1;
    
    std::cout << "vvedite vtoroe chislo: ";
    std::cin >> num2;
    
    max = (num1 > num2) ? num1 : num2;
    
    std::cout << "max chislo: " << max << std::endl;
    
}


void task_13() {
    bool value = true; 
    
    std::string result = (value) ? "yes" : "no";
    
    std::cout << "log znachenie : " << std::boolalpha << value << std::endl;
    std::cout << "preobrazov znachenie: " << result << std::endl;
    
}

