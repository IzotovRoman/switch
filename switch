#include <iostream>

void task_1() {
    std::cout << "enter num of week-day: ";
    int week_day = 0;
    std::cin >> week_day;
    
    const int Fr = 5;
    
    switch (week_day) {
        case 1:
            std::cout << "Md" << std::endl;
            
            break;
        case 2:
            std::cout << "Th" << std::endl;
            break;
        case 3:
            std::cout << "Wn" << std::endl;
            break;
        case 4:
            std::cout << "Th" << std::endl;
            break;
        case 5:
            std::cout << "Fr" << std::endl;
            break;
        case 6:
            std::cout << "Sd" << std::endl;
            break;
        case 7:
            std::cout << "Sun" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        
   }
    
}

void task_2(){
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    switch (sym) {
        case '+':
            std::cout << "+plus" << std::endl;
            break;
        case '-':
            std::cout << "minys-" << std::endl;
            break;
        case '*':
            std::cout << "*ymnozh" << std::endl;
            break;
        case '/':
            std::cout << "/podelit" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        }
    
}

void task_3(){
    std::cout << "enter SYM: ";
    char sym;
    std::cin >> sym;
    
    switch (sym) {
        case '5':
            std::cout << "otlichno" << std::endl;
            break;
        case '4':
            std::cout << "horosho" << std::endl;
            break;
        case '3':
            std::cout << "*norm" << std::endl;
            break;
        case '2':
            std::cout << "ploho" << std::endl;
            break;
        default:
            std::cout << "error" << std::endl;
        }
    
}



void task_10(){
    char symbol;
    
    
    std::cout << "vvedi simvol: ";
    std::cin >> symbol;
    
    
    switch (symbol) {
        // Проверка на цифры от 0 до 9
        case '0':
        case '1':
        case '2':
        case '3':
        case '4':
        case '5':
        case '6':
        case '7':
        case '8':
        case '9':
            std::cout << "yavl cifroy" << std::endl;
            break;
            
        
        case 'a':
        case 'b':
        case 'c':
        case 'd':
        case 'e':
        case 'f':
        case 'g':
        case 'h':
        case 'i':
        case 'j':
        case 'k':
        case 'l':
        case 'm':
        case 'n':
        case 'o':
        case 'p':
        case 'q':
        case 'r':
        case 's':
        case 't':
        case 'u':
        case 'v':
        case 'w':
        case 'x':
        case 'y':
        case 'z':
            std::cout << "strochn" << std::endl;
            break;
            
        default:
            std::cout << "drygoy simvol" << std::endl;
            break;
    }
    
    
}



